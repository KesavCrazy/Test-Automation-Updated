package StepDefinitions;

	import cucumber.api.PendingException;
	import cucumber.api.java.en.Given;
	import cucumber.api.java.en.When;
	import cucumber.api.java.en.Then;
	import cucumber.api.java.en.And;
	import cucumber.api.junit.Cucumber;
	import org.junit.runner.RunWith;

	@RunWith(Cucumber.class)
	public class CheckUp {

	    @Given("^User is on landing page$")
	    public void user_is_on_landing_page() throws Throwable {
	    	System.out.println("user landing page");
	    }

	    @When("^User login into application with (.+) and (.+)$")
	    public void user_login_into_application_with_and(String username, String password) throws Throwable {
	    	System.out.println("user_login_into_application");
	    }

	    @Then("^page is populated$")
	    public void page_is_populated() throws Throwable {
	    	System.out.println("page_is_populated");
	        
	    }
}
